<%for(var i=0; i<page.g_categories.length; i++) {
	var g_category = page.g_categories[i];%>
		<!--分类标题-->
        <h3><%=g_category.g_category%></h3>
	  	<%for(var j=0; j<g_category.g_projects.length; j++) {
			var g_project = g_category.g_projects[j];%>
			<!--仓库按钮-->
			<a href="//github.com/<%=g_project.g_user%>/<%=g_project.g_repo%>" role="button" target="_blank"><%=g_project.g_repo%></a>
			<!--描述-->
			<h5><%=g_project.g_description%></h5>
			<!--效果图-->
            <% if (g_project.g_images != null) {%>
			  	<% for(var k=0; k<g_project.g_images.length; k++) { 
					var g_image = g_project.g_images[k];%>
					<img src="<%=g_image%>"/>
				<% } %>
            <% } %>
		<% } %>
<% } %>